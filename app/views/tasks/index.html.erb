<h1>Listing tasks</h1>

<table>
  <tr>
    <th>Parent</th>
    <th>Project</th>
    <th>Tags</th>
    <th>Client</th>
    <th>Contact</th>
    <th>Name</th>
    <th>Status</th>
    <th>Focus</th>
    <th>Progress</th>
    <th>Notes</th>
    <th>Open date</th>
    <th>Start date</th>
    <th>Due date</th>
    <th>Close date</th>
    <th>Data</th>
    <th>State</th>
    <th>Sync</th>
    <th>Sync time</th>
    <th>Change time</th>
    <th>History</th>
    <th>Deleted</th>
    <th>Archived</th>
    <th>Active</th>
    <th>Locked</th>
    <th>Visible</th>
    <th>Audit log</th>
    <th>Permalog</th>
    <th>Priority</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @tasks.each do |task| %>
  <tr>
    <td><%= task.parent %></td>
    <td><%= task.project %></td>
    <td><%= task.tags %></td>
    <td><%= task.client %></td>
    <td><%= task.contact %></td>
    <td><%= task.name %></td>
    <td><%= task.status %></td>
    <td><%= task.focus %></td>
    <td><%= task.progress %></td>
    <td><%= task.notes %></td>
    <td><%= task.open_date %></td>
    <td><%= task.start_date %></td>
    <td><%= task.due_date %></td>
    <td><%= task.close_date %></td>
    <td><%= task.data %></td>
    <td><%= task.state %></td>
    <td><%= task.sync %></td>
    <td><%= task.sync_time %></td>
    <td><%= task.change_time %></td>
    <td><%= task.history %></td>
    <td><%= task.deleted_at %></td>
    <td><%= task.archived_on %></td>
    <td><%= task.active %></td>
    <td><%= task.locked_on %></td>
    <td><%= task.visible %></td>
    <td><%= task.audit_log %></td>
    <td><%= task.permalog %></td>
    <td><%= task.priority %></td>
    <td><%= link_to 'Show', task %></td>
    <td><%= link_to 'Edit', edit_task_path(task) %></td>
    <td><%= link_to 'Destroy', task, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

