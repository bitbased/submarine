<%= simple_form_for(@contact) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">

    <%= f.association :client %>

    <%= f.input :first_name %>
    <%= f.input :last_name %>
    <% #f.input :tags %>
    <%= f.input :company %>

    <%= f.input :notes %>

    <%= f.input :title %>
    <%= f.input :email %>
    <%= f.input :office_number %>
    <%= f.input :mobile_number %>
    <%= f.input :fax_number %>
    <%= f.input :shared %>
    <% #f.input :data %>
    <% #f.input :state %>
    <% #f.input :sync %>
    <% #f.input :sync_time %>
    <% #f.input :change_time %>
    <% #f.input :history %>
    <% #f.input :deleted_at %>
    <% #f.input :archived_on %>
    <% #f.input :active %>
    <% #f.input :locked_on %>
    <% #f.input :visible %>
    <% #f.input :audit_log %>
    <% #f.input :permalog %>
    <% #f.input :priority %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
