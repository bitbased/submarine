<h1>Listing time_entries</h1>

<table>
  <tr>
    <th>User</th>
    <th>Task Category</th>
    <th>Project</th>
    <th>Notes</th>
    <th>Timer Started At</th>
    <th>Date</th>
    <th>Idle</th>
    <th>Hours</th>
    <th>Billed</th>
    <th>Status</th>
    <th>Active</th>
    <th>Locked</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @time_entries.each do |time_entry| %>
  <tr>
    <td><%= time_entry.user %></td>
    <td><%= time_entry.task_category %></td>
    <td><%= time_entry.project %></td>
    <td><%= time_entry.notes %></td>
    <td><%= time_entry.timer_started_at %></td>

    <td><%= time_entry.date %></td>
    <td><%= time_entry.idle %></td>
    <td><%= time_entry.hours %></td>
    <td><%= time_entry.billed %></td>
    <td><%= time_entry.status %></td>
    <td><%= time_entry.data %></td>
    <td><%= time_entry.active %></td>
    <td><%= time_entry.locked_on %></td>
    <td><%= link_to 'Show', time_entry %></td>
    <td><%= link_to 'Edit', edit_time_entry_path(time_entry) %></td>
    <td><%= link_to 'Destroy', time_entry, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Time entry', new_time_entry_path %>
